const mongoose = require('mongoose');

const restaurantSchema = new mongoose.Schema({
  name: String,
  category: String,
  location: String,
  state: String,
  //typ f√ºr foto nachgucken.
  pic: String,
  reviews: Number,
  starRating: String,

  //text der bewertung - kommentar.
  description: String
});

const Restaurant = mongoose.model('Restaurant', restaurantSchema);

module.exports = Restaurant;